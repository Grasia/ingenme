       @@@/repeat@@@    
       @@@repeat id="onlypackage"@@@
           <item name="@@@v@@@name@@@/v@@@" href=""/>
       @@@/repeat@@@   				
    @@@repeat id="level"@@@ </item> @@@/repeat@@@	   				
  @@@/repeat@@@    
  </item>
 </menu>
        <menu ref="reports"/>
    </body>
</project>
