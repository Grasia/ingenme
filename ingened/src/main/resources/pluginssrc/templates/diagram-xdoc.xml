@@@program @@@
	@@@repeat id="graph1"@@@
		@@@saveto@@@
			@@@file overwrite="yes"@@@
				@@@v@@@output@@@/v@@@/xdoc/@@@v@@@fname@@@/v@@@.xml@@@/file@@@
			@@@text@@@
<document xmlns="http://maven.apache.org/XDOC/2.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/XDOC/2.0 http://maven.apache.org/xsd/xdoc-2.0.xsd">
  <properties>
    <title>Diagram @@@v@@@name@@@/v@@@</title>
  </properties>
  <body>
	<center><a href="index.html">Main Index</a></center>
	<hr/>
	
<section name="@@@v@@@name@@@/v@@@:@@@v@@@tipo@@@/v@@@">
<subsection name="Description">
@@@v@@@description@@@/v@@@ 
<div align="center"><img src="images/@@@v@@@image@@@/v@@@" usemap="#mymap"/>
<map name="mymap">
  @@@repeat id="mapentities"@@@
   <area shape="rect" coords="@@@v@@@coordrect@@@/v@@@" href="#@@@v@@@link@@@/v@@@"/>
  @@@/repeat@@@
</map></div>
</subsection>
<subsection name="Entities appearing in this diagram">
	<table border="1" >
	<thead>
      <tr>
       <th>NAME</th>
	   <th>TYPE</th>       
	   <th>DESCRIPTION</th>           
      </tr>
  </thead>
	
	 @@@repeat id="entities"@@@
	<tr><td valign="top">
	<a name="@@@v@@@name@@@/v@@@"/>
	  <strong> @@@v@@@name@@@/v@@@</strong><br/>
	</td><td valign="top">
	<strong>@@@v@@@tipo@@@/v@@@</strong>
	</td><td valign="top">
	@@@v@@@description@@@/v@@@
		<p> It appears in diagrams:
		<ul>
		@@@repeat id="relateddiagrams"@@@
		 <li><a href="@@@v@@@diagramname@@@/v@@@.html">@@@v@@@diagramname@@@/v@@@</a>: @@@v@@@diagramtype@@@/v@@@ </li>
		@@@/repeat@@@
		</ul>
			</p>
	</td></tr>
	 @@@/repeat@@@
	</table>
	</subsection>
	
<subsection name="Relationships appearing in this diagram">
	
	<table >
	<thead>
      <tr>
       <th>TYPE</th>
	   <th>SOURCE</th>       
	   <th>TARGET</th>           
      </tr>
  </thead>

	 @@@repeat id="entities"@@@
             	 @@@repeat id="relationship"@@@	   
                   <tr><td valign="top">
	             @@@v@@@name@@@/v@@@<br/>
	             @@@repeat id="roles"@@@
	                 </td><td valign="top">
	                 @@@v@@@player@@@/v@@@
	              @@@/repeat@@@
	                </td></tr>
       	 @@@/repeat@@@
             
       @@@/repeat@@@
	</table>
	</subsection>
	</section>
</body>
</document>
@@@/text@@@
		@@@/saveto@@@
	@@@/repeat@@@
@@@/program@@@

